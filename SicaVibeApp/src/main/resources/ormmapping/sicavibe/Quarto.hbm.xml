<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="sicavibe.Quarto" table="Quarto" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<many-to-one name="tipoDeQuarto" column="TipoDeQuartoID" class="sicavibe.TipoDeQuarto" cascade="save-update,lock" unique="true" not-null="true" lazy="proxy" access="field">
		</many-to-one>
		<property name="nPorta" column="NPorta" type="integer" length="10" not-null="true" lazy="false"/>
		<property name="estado" column="Estado" type="integer" length="10" not-null="true" lazy="false"/>
	</class>
</hibernate-mapping>
